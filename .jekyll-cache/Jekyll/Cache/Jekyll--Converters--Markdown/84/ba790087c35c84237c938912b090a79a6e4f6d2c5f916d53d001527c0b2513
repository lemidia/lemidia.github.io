<span class="nc">Adjacency</span> <span class="n">list</span> <span class="n">of</span> <span class="n">vertex</span> <span class="mi">1</span> <span class="o">=&gt;</span> <span class="mi">0</span> <span class="o">=&gt;</span> <span class="mi">3</span> <span class="o">=&gt;</span> <span class="mi">4</span>
<span class="nc">Adjacency</span> <span class="n">list</span> <span class="n">of</span> <span class="n">vertex</span> <span class="mi">2</span> <span class="o">=&gt;</span> <span class="mi">4</span> <span class="o">=&gt;</span> <span class="mi">0</span>
<span class="nc">Adjacency</span> <span class="n">list</span> <span class="n">of</span> <span class="n">vertex</span> <span class="mi">3</span> <span class="o">=&gt;</span> <span class="mi">1</span> <span class="o">=&gt;</span> <span class="mi">5</span> <span class="o">=&gt;</span> <span class="mi">4</span>
<span class="nc">Adjacency</span> <span class="n">list</span> <span class="n">of</span> <span class="n">vertex</span> <span class="mi">4</span> <span class="o">=&gt;</span> <span class="mi">5</span> <span class="o">=&gt;</span> <span class="mi">2</span> <span class="o">=&gt;</span> <span class="mi">1</span> <span class="o">=&gt;</span> <span class="mi">3</span>
<span class="nc">Adjacency</span> <span class="n">list</span> <span class="n">of</span> <span class="n">vertex</span> <span class="mi">5</span> <span class="o">=&gt;</span> <span class="mi">3</span> <span class="o">=&gt;</span> <span class="mi">4</span>

