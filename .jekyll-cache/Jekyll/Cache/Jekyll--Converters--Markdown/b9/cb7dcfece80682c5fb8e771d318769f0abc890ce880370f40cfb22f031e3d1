  <span class="n">printf</span><span class="p">(</span><span class="s">"[нѓђмѓ‰н•њ м •м ђ м€ %dк°њ]</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">counter</span><span class="p">);</span> 
