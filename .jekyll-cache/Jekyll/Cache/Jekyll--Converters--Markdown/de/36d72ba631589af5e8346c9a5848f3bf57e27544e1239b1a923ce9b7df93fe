    <span class="kt">int</span> <span class="n">top</span><span class="o">;</span> 
    <span class="kt">int</span> <span class="n">a</span><span class="o">[]</span> <span class="o">=</span> <span class="k">new</span> <span class="kt">int</span><span class="o">[</span><span class="no">MAX</span><span class="o">];</span> <span class="c1">// Maximum size of Stack </span>
  
    <span class="kt">boolean</span> <span class="nf">isEmpty</span><span class="o">()</span> 
    <span class="o">{</span> 
