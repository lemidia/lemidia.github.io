    <span class="o">}</span> 
    <span class="k">else</span> <span class="o">{</span> 
        <span class="kt">int</span> <span class="n">x</span> <span class="o">=</span> <span class="n">a</span><span class="o">[</span><span class="n">top</span><span class="o">--];</span> 
        <span class="k">return</span> <span class="n">x</span><span class="o">;</span> 
    <span class="o">}</span> 
<span class="o">}</span> 
